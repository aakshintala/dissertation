sudo /home/aakshintala/qemu-vgpu/x86_64-softmmu/qemu-system-x86_64 -m 4000 -drive format=raw,file=/home/aakshintala/vm-image/dev.img,if=virtio,cache=writeback -smp 4 --nographic -netdev user,id=net0,net=192.168.122.0/24,dhcpstart=192.168.122.10,hostfwd=tcp::2222-:22 -device e1000,netdev=net0,mac=18:03:00:00:00:01 -qmp unix:/tmp/qmp_run.sh,server,nowait -enable-kvm -machine accel=kvm -cpu host,kvm=on -device scea-vgpu

